    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "pinia": "^2.1.7",
    "url-join": "^5.0.0",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0",
    "vuetify": "^3.6.13"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.3",
    "@types/file-saver": "^2.0.7",
    "@types/lodash": "^4.17.7",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.27.0",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "typescript": "^5.5.3",
    "vite": "^5.3.4",
    "vite-plugin-vuetify": "^2.0.3",
    "vue-tsc": "^2.0.24"
